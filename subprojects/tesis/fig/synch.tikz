  \begin{lrbox}{\dropbox}
    \begin{tikzpicture}
      \draw[->] (-1.5,0.5cm) -- (-1.5,-2cm) ;
      \node at (-1.8,-1.3cm) {$t$};
      \node[frame] at (0,0cm)    (b0) {};
