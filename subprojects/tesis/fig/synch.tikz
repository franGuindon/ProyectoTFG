  \end{lrbox}

  \node[terminal] at (0,0cm) (src) {Base Video Frame};
  \node[terminal, xshift=-0.5cm, right=of src] (src1) {Video Frame with Artifacts};
  \coordinate[yshift=-1.5cm] (codo0) at ($(src)!0.5!(src1)$) {};
  \node[nonterminal, yshift=0.3cm, below=of codo0, align=center] (cmp) {
    Frame Timestamp Comparison \\ 
    \vspace*{0.1cm} \\
    \usebox\cmpbox
  };
  \node[nonterminal, right=of cmp, align=center] (drop) {
    Unmatched Base Frame Drop \\ 
    \vspace*{0.1cm} \\
    \usebox\dropbox
  };

  \coordinate[mux, yshift=-3cm] (codo00) at ($(drop)!0.5!(cmp)$) {};

  \coordinate[] (codo1) at ($(codo00.south)+(0,-0.5cm)$) {};
  \coordinate[] (codo2) at ($(codo1)+(-2.8,0cm)$) {};
  \coordinate[] (codo3) at ($(codo1)+(2.8,0cm)$) {};
  \node[terminal, below=of codo2, align=center] (dst) {Synchronized \\ Base Video Frame};
  \node[terminal, below=of codo3] (dst1) {Video Frame with Artifacts};

  \draw (src) |- (codo0) -| (src1);
  \draw[->] (codo0) -- (cmp);
  \draw[->] (cmp) -- (drop);
  \draw[->] (cmp) |- (codo00);
  \draw[->] (drop) |- (codo00);
  \draw (codo00) -- (codo1);
  \draw[<->] (dst) |- (codo1) -| (dst1);

  \draw[line width=0.6mm] ($0.5*(cmp)+0.5*(drop)$) ++ (0,0.3cm) ++
        (-0.08,0.08cm) -- ++ (0.16,-0.16cm) ++
        (0,0.16cm) -- ++ (-0.16,-0.16cm);
  
  \draw[line width=0.6mm] (cmp.south) ++ (-0.3,-0.4cm) ++ 
        (-0.1,-0.02cm) -- ++ (0.1,-0.08cm) -- ++ (0.1,0.2cm);

  \scoped[on background layer]{

    \node [draw, rectangle, fit=(codo0)(cmp)(codo1)(drop), fill=orange!30,
      inner sep=0.3cm] (system) {};

    \node [anchor=south east, inner sep=0.3cm] at (system.north east) () {
      Video Synchronizer};
      
  }
  
