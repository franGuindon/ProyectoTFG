      \node[frame, dashed] at (-0.2,-1.2cm) (b4) {};
      \node[frame] at (0,-1.5cm) (b5) {};

      \node[frame] at (2,0cm)    (d0) {};
      \node[frame] at (2,-0.3cm) (d1) {};
      \node[frame] at (2,-0.6cm) (d2) {};
      \node[missing_frame] at (2,-0.9cm) (d3) {};
      \node[missing_frame] at (2,-1.2cm) (d4){};
      \node[frame] at (2,-1.5cm) (d5) {};

