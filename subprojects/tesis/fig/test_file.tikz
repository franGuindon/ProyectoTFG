\begin{document}
\begin{tikzpicture}[node distance=1cm, line width=0.4mm]

\node[terminal] at (0,0cm) (src) {Video Source};
\node[nonterminal, below=of src] (encoder) {Video Encoding System};
\node[nonterminal, right=of encoder, align=center] (channel) {Video Transmission\\(with packet losses)};
\node[nonterminal, right=of channel] (decoder) {Video Decoding System};
\node[terminal, below=of decoder] (dst) {Video with Artifacts};

\draw[->] (src) -- (encoder);
\draw[->] (encoder) -- (channel);
\draw[->] (channel) -- (decoder);
\draw[->] (decoder) -- (dst);

% Method to draw (down right down) or (down left down) arrows between nodes:
